{
    "cmd": "/bin/containerbuddy -config file:///etc/containerbuddy/containerbuddy.json nginx -g \"daemon off;\"",
    "container": {
        "docker": {
            "image": "${env.IMAGE_PREFIX}/ap-nginx",
            "network": "HOST"
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "env": {
        "CONSUL_HTTP_SSL_VERIFY": "true"
    },
    "forcePullImage": "true",
    "id": "${env.IMAGE_PREFIX}/ap-nginx",
    "instances": 1,
    "mem": 128
}
